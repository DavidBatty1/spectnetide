<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"       
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:userControls="clr-namespace:Spect.Net.Z80Tests.UserControls" 
             xmlns:debug="clr-namespace:Spect.Net.Z80Tests.ViewModels.Debug"
             x:Name="UserControl"       
             x:Class="Spect.Net.Z80Tests.Views.DisassemblyView"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" d:DataContext="{d:DesignInstance {x:Type debug:DisassemblyViewModel}}">
    <Grid>
        <userControls:DisassemblyListControl DisassemblySource="{Binding DisassemblyItems}" />
    </Grid>
</UserControl>
